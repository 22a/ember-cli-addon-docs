<div class="docs-viewer__nav-search">
  <input type="text" placeholder="Type to search" oninput={{action 'search' value='target.value'}}>
</div>

{{#docs-viewer/x-nav-list}}
  {{yield (hash
    item=(component 'docs-viewer/x-nav-item')
    subnav=(component 'docs-viewer/x-nav-list')
  )}}

  {{!-- Autogenerated API docs --}}
  {{docs-viewer/x-nav-item 'API Reference' 'docs.api.index'}}

  {{#docs-viewer/x-nav-list as |subnav|}}
    {{#each projectVersion.sortedClasses as |class|}}
      {{subnav.item class.name 'docs.api.class' class.id}}
    {{/each}}
  {{/docs-viewer/x-nav-list}}
{{/docs-viewer/x-nav-list}}
